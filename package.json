{
  "name": "@plumejs/router",
  "version": "3.0.1",
  "description": "A routing mechanism for PlumeJs application.",
  "files": [
    "dist/**/*"
  ],
  "main": "dist/cjs/index.js",
  "module": "dist/mjs/index.js",
  "exports": {
    ".": {
      "import": "./dist/mjs/index.js",
      "require": "./dist/cjs/index.js"
    }
  },
  "types": "./dist/index.d.ts",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "npm run lint && rimraf dist && tsc -p tsconfig.json && tsc -p tsconfig-cjs.json",
    "lint": "eslint . --ext .ts --fix",
    "patch-update": "npm version patch",
    "tag": "git push --follow-tags",
    "tag-release": "npm run patch-update && npm run tag"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/KiranMantha/plumejs-router.git"
  },
  "keywords": [
    "plumejs-router",
    "plumejs",
    "router"
  ],
  "author": "KiranMantha",
  "license": "SEE LICENSE IN LICENSE",
  "bugs": {
    "url": "https://github.com/KiranMantha/plumejs-router/issues"
  },
  "homepage": "https://github.com/KiranMantha/plumejs-router#readme",
  "dependencies": {
    "@plumejs/core": "^3.1.0"
  },
  "devDependencies": {
    "@types/node": "^16.11.9",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "eslint": "^8.2.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "prettier": "^2.4.1",
    "rimraf": "^3.0.2",
    "rxjs": "^7.5.2",
    "ttypescript": "^1.5.12",
    "typescript": "^4.5.2"
  }
}
